# E2E Test Environment Variables

# Gemini API Key (required for AI tests)
GEMINI_API_KEY=your_gemini_api_key_here

# Database URL for tests
DATABASE_URL=postgresql://test_user:test_pass@localhost:5433/dnd_test

# Frontend URL
FRONTEND_URL=http://localhost:5173

# Backend URL
BACKEND_URL=http://localhost:5000

# Test user credentials (will be created during tests)
TEST_USER_EMAIL=test-wizard@example.com
TEST_USER_PASSWORD=TestPassword123!

# Playwright options
SLOW_MO=0  # Set to 100 for slow motion debugging
HEADLESS=true  # Set to false to see browser
